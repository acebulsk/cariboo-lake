file = {:mnt/7A40CC1F40CBDFCD/OneDrive - University of Saskatchewan/PhD/courses/GEO827/readings/snowreadings/snowreadings/ofinterest/kuchmentgelfan_selfsimilarsnow.pdf:pdf},
