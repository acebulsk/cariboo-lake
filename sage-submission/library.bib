file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Clark et al. - 2014 - Effects of a small-scale, abandoned gold mine on the geochemistry of fine stream-bed and floodplain sediments i(2).pdf:pdf},
issn = {0026461X},
journal = {Mineralogical Magazine},
number = {6},
pages = {1491--1504},
title = {{Effects of a small-scale, abandoned gold mine on the geochemistry of fine stream-bed and floodplain sediments in the Horsefly River watershed, British Columbia, Canada}},
url = {http://openurl.ingenta.com/content/xref?genre=article&issn=0026-461X&volume=78&issue=6&spage=1491},
