journal = {Canadian journal of forest research},
keywords = {Abies lasiocarpa,Picea engelmannii,Pinus contorta,bending,branches,crystal bouncing,elastic rebound,habit,interception,snow,snow bridging},
number = {8},
pages = {1262--1269},
title = {{Snowfall interception on branches of three conifer species}},
volume = {21},
year = {1991}
}
