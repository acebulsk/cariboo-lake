file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pomeroy, Hanson, Faria - 2001 - Small-Scale Variation in Snowmelt Energy in a Boreal Forest An Additional Factor Controlling Depletion.pdf:pdf},
