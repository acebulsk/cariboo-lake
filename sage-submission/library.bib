language = {eng},
publisher = {Western Snow Conference},
title = {{Interception and redistribution of snow in a subalpine forest on a storm-by-storm basis}},
url = {http://sites/westernsnowconference.org/PDFs/1987Wheeler.pdf},
year = {1987}
}
