keywords = {Environmental Sciences,Environmental Sciences & Ecology,LiDAR,Life Sciences & Biomedicine,Limnology,Marine & Freshwater Biology,Physical Sciences,Science & Technology,Water Resources,forest,silviculture,snow,snow depletion curves},
number = {11},
