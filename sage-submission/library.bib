@article{Leonard1999,
abstract = {Sediment cores from three proglacial lakes in northern Banff National Park, Alberta, preserve a record of Holocene glacial activity upvalley which is more continuous and better dated than available surficial records. Dating of the cores is based on tephrochronology and 16 AMS14C ages of terrestrial macrofossils. All cores contain a threefold sequence of lacustrine sediments overlying a late Pleistocene diamicton. Basal lacustrine sediments > 10,10014C yr old contain little organic matter. Sediment composition indicates a large glacigenic contribution. A sharp increase in organic content marks the beginning of the Altithermal interval at all three lakes. This transition occurred abruptly at about 10,10014C yr B.P. at Crowfoot Lake and possibly more gradually at the other lakes. Altithermal sediments contain relatively little glacigenic material, and during most of the Altithermal, glaciers may have been absent above Crowfoot and Bow Lakes. Glaciers draining into Hector Lake appear to have persisted through the Altithermal. A subsequent decrease in organic content in each lake, reflecting increased clastic sedimentation, marks the end of the Altithermal and the onset of Neoglacial ice advances. The transition took place between about 5800 and 400014C yr B.P. and may be time-transgressive, beginning earlier in Hector Lake than in Crowfoot Lake. Changing Neoglacial clastic sedimentation rates through the Neoglacial interval indicate two main periods of increased glacier extent, between ca. 3000 and 1800 varve yr ago (ca. 2900-190014C yr B.P.) and during the last several hundred years. During the intervening period glaciers were less extensive, but much more extensive than during the recessions of the Altithermal interval.},
author = {Leonard, Eric M. and Reasoner, Mel A.},
doi = {10.1006/qres.1998.2009},
file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Leonard, Reasoner - 1999 - A continuous holocene glacial record inferred from proglacial lake sediments in Banff National Park, Alber(2).pdf:pdf;:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Leonard, Reasoner - 1999 - A Continuous Holocene Glacial Record Inferred from Proglacial Lake Sediments in Banff National Park, Alberta,.pdf:pdf},
isbn = {0033-5894},
issn = {00335894},
journal = {Quaternary Research},
keywords = {Altithermal,Canadian Rocky Mountains,Glacial history,Glaciolacustrine sedimentation,Holocene},
month = {jan},
number = {1},
pages = {1--13},
publisher = {No longer published by Elsevier},
title = {{A continuous holocene glacial record inferred from proglacial lake sediments in Banff National Park, Alberta, Canada}},
url = {https://www.sciencedirect.com/science/article/pii/S0033589498920099},
volume = {51},
year = {1999}
}
@article{Gray1979a,
abstract = {The paper reviews the "state-of-the-art" of methods of estimating t h e a r e a l water equivalent of a shallow P r a i r i e snowcover. The use of snowcover d a t a a r e assessed and evaluated with respect t o the needs of operational forecasting agencies with special emphasis on basin yield and streamflow prediction. Data a r e provided on t h e r e l a t i v e value of Fischer and Porter and MSC Nipher p r e c i p i t a t i o n gauges for estimating snow water equivalent. However, the paper s t r e s s e s t h e u t i l i t y of using landscape course snow surveys f o r obtaining r e l i a b l e estimates of t h e t r u e mean basin water equivalent. Data from comprehensive snow surveys are presented t o i l l u s t r a t e the v a r i a b i l i t y i n snowcover properties of landscape groups which have been differentiated on t h e basis of t e r r a i n , vegetative cover and land use features. In addition, t h e sampling procedures t o be followed t o obtain s t a t i s t i c a l l y-v a l i d estimates of depth and density within t h e d i f f e r e n t landscape types a r e presented. Comparisons between ' t h e mean snow water equivalent of different landscape types t o M S C Nipher measurements near t h e time of maximum accumulation are tabulated. A new approach for determining the mean areal water equivalent which combines t h e use of Landsat imagery, snow survey data and measurements from MSC Nipher precipitation gauges is discussed. An attempt i s made t o demonstrate the s e n s i t i v i t y of snowmelt output from t h e U.S. National Weather Service Sacramento Model t o s p a t i a l l y variable snow water equivalent accuinulations over a typical P r a i r i e watershed.},
author = {Gray, Don M and Steppuhn, H and Abbey, F. L.},
file = {:mnt/7A40CC1F40CBDFCD/OneDrive - University of Saskatchewan/PhD/courses/GEO827/readings/snowreadings/snowreadings/ofinterest/Gray_et_al_1979arealSWEprairies.pdf:pdf},
