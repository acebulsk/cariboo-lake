file = {:mnt/SSD/OneDrive - University of Saskatchewan/PhD/papers/pomeroy_et_al_1998_3.pdf:pdf;:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pomeroy et al. - 1998 - An evaluation of snow accumulation and ablation processes for land surface modelling.pdf:pdf},
