doi = {10.1002/2015RG000481},
file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kinar, Pomeroy - 2015 - Measurement of the physical properties of the snowpack.pdf:pdf},
