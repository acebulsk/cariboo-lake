number = {D24},
pages = {28901--28909},
publisher = {Blackwell Publishing Ltd},
title = {{Albedo over the boreal forest}},
volume = {102},
year = {1997}
}
@article{Petticrew2015a,
abstract = {On 4 August 2014, a catastrophic breach of the Mount Polley mine tailings impoundment released $\sim$25 M m3 of tailings and water and scoured an unknown quantity of overburden into the West Basin of Quesnel Lake. We document Quesnel Lake and Quesnel River observations for 2 months postspill. Breach inflows raised Quesnel Lake by 7.7 cm, equivalent to $\sim$21 M m3. The West Basin hypolimnion was modified immediately, exhibiting increased temperature ($\sim$5°C to 6–7.5°C), conductivity (110 to 160 $\mu$S/cm), and turbidity (<1 to 200–1000 nephelometric turbidity units (NTU)). Cooscillating seiches moved West Basin hypolimnetic water both westward and eastward contaminating the Main Basin. Postspill, high-turbidity water propagated eastward ($\sim$1 cm/s), introducing a persistent $\sim$20 m thick layer below the thermocline and an $\sim$30 m thick layer at the bottom. The contaminant introduction, mobilization, and bioaccumulation may pose risks to resident and anadromous fish stocks, which support recreational, commercial, and First Nations fisheries.},
author = {Petticrew, Ellen L. and Albers, Sam J. and Baldwin, Susan A. and Carmack, Eddy C. and D{\'{e}}ry, Stephen J. and Gantner, Nikolaus and Graves, Kelly E. and Laval, Bernard and Morrison, John and Owens, Philip N. and Selbie, Daniel T. and Vagle, Svein},
doi = {10.1002/2015GL063345},
isbn = {1944-8007},
issn = {19448007},
journal = {Geophysical Research Letters},
keywords = {ecosystem response,metal bioaccumulation,mine disaster,physical limnology,sediment loading,turbidity current},
number = {9},
pages = {3347--3355},
title = {{The impact of a catastrophic mine tailings impoundment spill into one of North America's largest fjord lakes: Quesnel Lake, British Columbia, Canada}},
volume = {42},
year = {2015}
}
