issn = {0885-6087},
journal = {Hydrological processes},
keywords = {Analysis,Canopy snow interception,Coniferous forests,Deciduous forests,Energy balance,Mass balance,Melt,Models,Physical Sciences,Science & Technology,Unloading,Water Resources},
