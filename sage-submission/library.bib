journal = {Hydrological processes},
keywords = {Earth,Earth sciences,Exact sciences and technology,Frozen soils,Hydrology,Hydrology. Hydrogeology,Infiltration,Numerical simulation,Parametric equation,Soils,Surficial geology,ocean,space},
