@incollection{King2008,
abstract = {Climates at the global, the regional, and the local scale determine the relative contributions of radiation, turbulent and mass fluxes to the corresponding balance at the atmosphere-ground interface. Presence of a snow cover changes the physical properties of the atmosphere-ground interface, which in turn affects the above fluxes, thereby influencing the properties of the lower atmosphere. In addition, fluxes involved at the snow-atmosphere interface depend much on the ground vegetation, on the snow cover being continuous or patchy as well as on topography. This chapter first presents general equations for both energy and mass balances (Section 3.2). Detailed discussions of each component of the energy balance follow (Section 3.3), including effects due to either vegetation or topography. Next, the influence of vegetation, blowing snow, as well as topography, on snow accumulation is treated (Section 3.4), including some considerations of the coupling of energy and mass balances. Finally, in order to give the reader a sense of the magnitude of the fluxes involved, some examples are presented (Section 3.5), considering either daily averages over typical periods (season) or the processes in detail. Energy balance is often formulated in terms of energy exchanges taking place at an interface (see e.g. Oke, 1987). Such an active surface has to be thought of as being infinitesimally thin and having neither mass nor specific heat. Penetration Snow and Climate: Physical Processes, Surface Energy Exchange and Modeling, ed. Richard L. Armstrong and Eric Brun.},
author = {King, John C and Pomeroy, J. W. and Gray, Donald M and Fierz, Charles and F{\"{o}}hn, Paul M B and Harding, Richard J and Jordan, Rachel E and Martin, Eric and Pl{\"{u}}ss, Christian},
booktitle = {Snow and Climate: Physical Processes, Surface Energy Exchange and Modelling},
chapter = {3},
edition = {Richard L.},
file = {:home/alex/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/King et al. - 2007 - Snow-atmosphere energy and mass balance.pdf:pdf},
pages = {70--123},
publisher = {Cambridge University Press},
title = {{Snow-atmosphere energy and mass balance}},
year = {2008}
}
@article{Singh1997,
abstract = {Thirteen equations based on the mass-transfer method for determining free water evaporation were expressed in seven generalized equations. These seven equations were then compared with pan evaporation at four climatological stations in north-western Ontario, Canada. The comparisons were based on monthly evaporation. Equations were compared by calibrating them on the entire data sets as well as by calibrating on part of the data and then verifying them on the remainder of the data. The results of comparison showed that all equations were in reasonable agreement with observed evaporation, and that the effect of wind velocity on monthly evaporation was marginal. However, when an equation with parameters obtained at one site was applied to compute evaporation at another site, the computed evaporation was not in good agreement with observed values. {\textcopyright} 1997 by John Wiley & Sons, Ltd.},
