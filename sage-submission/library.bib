doi = {10.1002/(SICI)1099-1085(199910)13:14/15<2423::AID-HYP853>3.0.CO;2-U},
file = {:mnt/7A40CC1F40CBDFCD/OneDrive - University of Saskatchewan/PhD/courses/GEO827/readings/snowreadings/snowreadings/ofinterest/distributedblowingsnow.pdf:pdf},
