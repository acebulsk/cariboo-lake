issn = {1525-755X},
journal = {Journal of hydrometeorology},
keywords = {Agrology,Aircraft,Carbon dioxide,Corn,Covariance,Hydrometeorology,Meteorology & Atmospheric Sciences,NOTES AND CORRESPONDENCE,Physical Sciences,Prairie soils,Science & Technology,Soil water,Soybeans,Water vapor},
number = {6},
pages = {954--960},
