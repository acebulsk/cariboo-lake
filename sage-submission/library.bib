author = {Brun, E and Martin, Ε and Simon, V and Gendre, C and Coleou, C},
issn = {0022-1430},
journal = {Journal of glaciology},
keywords = {Earth,Exact sciences and technology,External geophysics,Geography,Geology,Geosciences,Multidisciplinary,Physical,Physical Geography,Physical Sciences,Science & Technology,Snow. Ice. Glaciers,ocean,space},
number = {121},
