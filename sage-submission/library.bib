issn = {00344257},
journal = {Remote Sensing of Environment},
keywords = {Imaging spectrometry,Remote sensing,Snow},
number = {SUPPL. 1},
pages = {S25--S37},
publisher = {Elsevier Inc.},
title = {{Interpretation of snow properties from imaging spectrometry}},
url = {http://dx.doi.org/10.1016/j.rse.2007.07.029},
volume = {113},
