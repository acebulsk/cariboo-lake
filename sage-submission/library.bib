issn = {00221694},
journal = {Journal of Hydrology},
keywords = {Canadian hydrometric network,Entropy theory,Network design,Transinformation index},
number = {3-4},
pages = {420--437},
publisher = {Elsevier B.V.},
title = {{Hydrometric network evaluation for Canadian watersheds}},
url = {http://dx.doi.org/10.1016/j.jhydrol.2009.11.015},
volume = {380},
year = {2010}
}
@article{Cockburn2007,
abstract = {Formation of annually laminated sediments in Summit Lake, White Pass, British Columbia is controlled by runoff generated by snowpack and glacier melt and major rainfall events. The 700-yr varve record is divided into two subannual series (early and late) based on sedimentological criteria and sedimentary structures within each varve. A comparison of recent subannual laminae with nearby meteorological records supports the interpretation they are formed by river discharge events generated by major snow and glacier melt events and large late-summer rainfall events. A significant correlation exists between the late subannual thickness series and the size of the largest rainfall events in late summer. The long record indicates there was an abrupt increase in the thickness and frequency of major rainfall-induced sedimentary events at the end of the seventeenth century. In addition, the frequency of laminae generated by early runoff events also increased. However, early subannual varve thickness component remains statistically the same as the thickness prior to the end of the seventeenth century. This suggests the change in varve thickness at this time is due to increases in major late-summer rainfall frequency rather than increased sediment availability caused by regional Little Ice Age glacier advances. {\textcopyright} 2006 University of Washington.},
author = {Cockburn, Jaclyn M.H. and Lamoureux, Scott F.},
doi = {10.1016/j.yqres.2006.10.003},
file = {::},
issn = {00335894},
journal = {Quaternary Research},
keywords = {British Columbia,Climate change,Little Ice Age,Precipitation,Subannual sedimentary units,Varves},
number = {2},
pages = {193--203},
title = {{Century-scale variability in late-summer rainfall events recorded over seven centuries in subannually laminated lacustrine sediments, White Pass, British Columbia}},
volume = {67},
year = {2007}
}
